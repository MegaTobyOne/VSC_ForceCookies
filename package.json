{
  "name": "force-cookies",
  "displayName": "Force Cookies",
  "description": "Clone wars era wisdom",
  "version": "0.0.1",
  "publisher": "TobyHarvey",
  "repository": {
    "type": "git",
    "url": "https://github.com/MegaTobyOne/VSC_ForceCookies"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "star wars",
    "clone wars",
    "wisdom",
    "quotes",
    "force"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "force-cookies.helloWorld",
        "title": "Force Cookies: Get Wisdom"
      },
      {
        "command": "force-cookies.showWisdom",
        "title": "Force Cookies: Show Wisdom"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
